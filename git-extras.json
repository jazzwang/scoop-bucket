{
    "version": "7.3.0",
    "license": "MIT",
    "extract_dir": "git-extras-7.3.0",
    "url":  "https://github.com/tj/git-extras/archive/refs/tags/7.3.0.zip",
    "description": "GIT utilities -- repo summary, repl, changelog population, author commit percentages and more",
    "homepage": "https://github.com/tj/git-extras",
    "notes": [
        "If you encounter error to install, please enable developer mode.",
        "run command within cmd.exe with Administrator permission:",
        "REG ADD \"HKEY_LOCAL_MACHINE\\SOFTWARE\\Microsoft\\Windows\\CurrentVersion\\AppModelUnlock" /t REG_DWORD /f /v \"AllowDevelopmentWithoutDevLicense\" /d \"1\""
    ],
    "post_install": [
        "pushd \"$dir\"",
        "try { & \"$dir\\install.cmd\" } catch { info $_.Exception.Message }",
        "popd"
    ]
}
